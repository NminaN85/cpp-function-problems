#include <iostream>
#include<string>
using namespace std;

void ReadNumbers(float& A, float& B ) {

    cout << "Enter A ?";
    cin >> A;
    cout << "Enter B ? ";
    cin >> B;
   
}

float calculateRectangleArea(float A, float B) {
    float Area;
    Area = A * B;
    return Area;
}

void PrintArea(float Area) {
    cout << "Rectangle Area = " << Area << endl;
}

int main()
{
    float A;
    float B;
    ReadNumbers(A, B);
    PrintArea(calculateRectangleArea(A, B));
    return 0;
}
