#include <iostream>   
#include <string>     

using namespace std;  


int ReadNumberN()
{
    int N;
    cout << "Please enter Number N ? " << endl;
    cin >> N;
    return N;
}

void PrintNUsingFor(int N) {
   int counter = 0; 
   for (int i = 1; i <= N; i++) {
       cout << i << endl;
      
   }
   cout << "----------------------------"<<endl;
}
void PrintNUsingWhile(int N) {
     int counter = 1;
    while (counter <=N ){
        cout << counter << endl;
        counter++;
    }
    
}

void PrintNUsingDo(int N) {

    int counter = 1;

    do {
        cout << counter << endl;
        counter++;
    } while (counter <= N);
}



int main()
{
    
    //PrintNUsingFor(ReadNumberN());
    /*PrintNUsingWhile(ReadNumberN());*/
    PrintNUsingDo(ReadNumberN());


    return 0;
}
